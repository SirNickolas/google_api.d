name        "google-api"
description "Unofficial bindings to Google APIs."
authors     "Nickolay Bukreyev"
copyright   "Copyright Â© 2024, Nickolay Bukreyev"
license     "MIT"

toolchainRequirements frontend=">=2.102"

subPackage "./extra-fastjwt"

dependency "vibe-http" version="~>1.0"
dependency "vibe-serialization" version="~>1.0"
dependency "vibe-stream" version="~>1.0"

targetPath "build"

configuration "library"

configuration "unittest" {
    sourcePaths "test"
    importPaths "test"

    dflags "-dip1000"
}
